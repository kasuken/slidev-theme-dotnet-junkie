{
  "name": "slidev-theme-dotnet-junkie",
  "version": "0.0.20",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.40.3",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.4",
    "theme-vitesse": "^0.6.2"
  },
  "devDependencies": {
    "@slidev/cli": "^0.40.3"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "dark",
    "highlighter": "prism",
    "defaults": {
      "aspectRatio": "16/9",
      "fonts": {
        "sans": "Roboto",
        "mono": "Cascadia Code"
      }
    }
  },
  "author": "Emanuele Bartolesi <emaueleb@outlook.com>",
  "bugs": "https://github.com/kasuken/sli-dev-dotnet-junkie/issues"
}
